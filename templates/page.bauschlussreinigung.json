    "about_section_v3_F8BxLh": {
      "type": "about-section-v3",
      "name": "Über Uns (Layout) – v3",
      "settings": {
        "image_position": "right",
        "image_vertical_align": "center",
        "show_image": false,
        "background_color": "#FFFFFF",
        "section_padding_top": 25,
