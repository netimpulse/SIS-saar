    "about_section_v3_TwMWej": {
      "type": "about-section-v3",
      "name": "Über Uns (Layout) – v3",
      "settings": {
        "image_position": "right",
        "image_vertical_align": "start",
        "show_image": true,
        "background_color": "#ffffff",
