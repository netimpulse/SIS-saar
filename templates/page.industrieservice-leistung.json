        "button_text_color_hover": "#ffffff"
      }
    },
    "text_mit_bild_button_QfRaj9": {
      "type": "text-mit-bild-button",
      "name": "Text mit Bild",
      "settings": {
        "image_position": "left",
        "image_shadow_strength": 20,
        "heading": "Fluidmanagement & Wartung",
        "heading_font": "sans-serif",
